{
    "autoload": {
        "psr-4": {
            "BestIt\\CtCustomerPricesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\BestIt\\CtCustomerPricesBundle\\": "tests/"
        }
    },
    "name": "bestit/commercetools-customer-prices-bundle",
    "description": "Provides a commercetools substitute with custom objects to get customer individual prices.",
    "type": "lib",
    "require": {
        "commercetools/php-sdk": "^1.4 || ^2.0",
        "php": "^7.0",
        "symfony/proxy-manager-bridge": "^3.0",
        "symfony/dependency-injection": "^3.2",
        "symfony/cache": "^3.2",
        "symfony/security": "^3.2",
        "symfony/stopwatch": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "matthiasnoback/symfony-dependency-injection-test": "^2.1",
        "matthiasnoback/symfony-config-test": "^3.0",
        "bestit/php_codesniffer": "^1.0"
    },
    "suggest": {
        "ocramius/proxy-manager": "Makes the price collection a lazy service."
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bjoern Lange",
            "email": "lange@bestit-online.de"
        }
    ],
    "minimum-stability": "beta"
}
